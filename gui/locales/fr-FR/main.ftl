## Exemples de pluralisation
users-count =
    { $count ->
        [0] Aucun utilisateur
        [one] 1 utilisateur
       *[other] { $count } utilisateurs
    }

items-remaining =
    { $count ->
        [0] Aucun élément restant
        [one] 1 élément restant
       *[other] { $count } éléments restants
    }

