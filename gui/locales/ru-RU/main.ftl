## Примеры множественного числа
users-count =
    { $count ->
        [0] Нет пользователей
        [one] { $count } пользователь
        [few] { $count } пользователя
       *[other] { $count } пользователей
    }

items-remaining =
    { $count ->
        [0] Нет оставшихся элементов
        [one] { $count } оставшийся элемент
        [few] { $count } оставшихся элемента
       *[other] { $count } оставшихся элементов
    }

